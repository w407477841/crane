<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xingyun.equipment.crane.modular.alipay.dao.ProjectTopUpMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xingyun.equipment.crane.modular.alipay.model.ProjectTopUp">
        <id column="id" property="id" />
        <result column="project_id" property="projectId" />
        <result column="project_name" property="projectName" />
        <result column="crane_no" property="craneNo" />
        <result column="device_no" property="deviceNo" />
        <result column="gprs" property="gprs" />
        <result column="charge_time" property="chargeTime" />
        <result column="charge_money" property="chargeMoney" />
        <result column="is_del" property="isDel" />
        <result column="create_time" property="createTime" />
        <result column="create_user" property="createUser" />
        <result column="modify_time" property="modifyTime" />
        <result column="modify_user" property="modifyUser" />
        <result column="status" property="status" />
        <result column="out_trade_no" property="outTradeNo" />

    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, project_id, project_name, crane_no, device_no, gprs, charge_time, charge_money, is_del, create_time, create_user, modify_time, modify_user,status
    </sql>

    <!-- 通用查询结果列(带别名)-->
    <sql id="Column_List">
        ${alias}.id,
        ${alias}.project_id,
        ${alias}.project_name,
        ${alias}.crane_no,
        ${alias}.device_no,
        ${alias}.gprs,
        ${alias}.charge_time,
        ${alias}.is_del,
        ${alias}.create_time,
        ${alias}.create_user,
        ${alias}.modify_time,
        ${alias}.modify_user,
        ${alias}.status,
    </sql>

    <select id="selectPageList" resultMap="BaseResultMap">
        SELECT
        <include refid="Column_List">
            <property name="alias" value="a" />
        </include>
        u.name as createUserName
        from  t_project_top_up a
        left join t_project_info b on a.project_id =b.id
        left join t_sys_user u on u.id=a.create_user
        <where>
            ${ew.sqlSegment}
        </where>
        order by a.id desc
    </select>

</mapper>
